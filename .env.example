# Copy this file to .env and fill in your Square credentials.
# The application ID and location ID are safe to share with the browser.
SQUARE_APPLICATION_ID=REPLACE_ME
SQUARE_LOCATION_ID=REPLACE_ME

# Keep the access token secret. Use your Sandbox token for testing
# and switch to a Production token when you are ready to go live.
SQUARE_ACCESS_TOKEN=REPLACE_ME

# Optional settings
SQUARE_ENVIRONMENT=sandbox
SQUARE_CURRENCY=USD
PORT=3000
